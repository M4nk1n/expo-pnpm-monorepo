{
  "presets": ["@expo/next-adapter/babel"],
  "plugins": [
    "react-native-web",
    // https://github.com/expo/expo/issues/14374
    ["@babel/plugin-proposal-private-methods", { "loose": true }],
    ["@babel/plugin-proposal-private-property-in-object", { "loose": true }],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@app": "../../packages/app/src"
        }
      }
    ]
  ]
}
